
class Student
{
public:

    Student() = default;

    void input()
    {
        for (int i = 0; i < 5; i++)
        {
            int num;
            cin >> num;
            score.push_back(num);
        }

    };

    int calculateTotalScore()
    {
        int sum = 0;
        for (auto& i : score)
        {
            sum += i;
        }
        return sum;
    };


    vector<int> score;
};
